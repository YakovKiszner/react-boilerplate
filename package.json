{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A simple react boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server/server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "start:prod": "npm run test && npm run build && npm run start",
    "start:dev": "cross-env NODE_ENV=development node server/server.js",
    "test": ""
  },
